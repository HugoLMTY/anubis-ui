[
  {
    "prefix": "blur",
    "standalone": true,
    "declaration": "backdrop-filter: blur(${value})",
    "variations": {
      "default": "3px"
    }
  },
  {
    "prefix": "smooth",
    "standalone": true,
    "declaration": "transition-duration: ${value}",
    "variations": {
      "default": "0.1s",
      "slowest": "0.5s",
      "slower": "0.3s",
      "slow": "0.2s",
      "quick": "0.07s",
      "quicker": "0.05s",
      "quickest": "0.03s"
    }
  },
  {
    "prefix": "rounded",
    "standalone": true,
    "declaration": "border-radius: ${value}",
    "variations": {
      "default": "8px",
      "square": "0px",
      "xs": "2px",
      "sm": "4px",
      "md": "8px",
      "lg": "12px",
      "xl": "16px",
      "very": "9999px",
      "full": "50%",
      "half": "100%"
    }
  },
  {
    "prefix": "border",
    "declaration": "border-style: ${value}",
    "variations": {
      "solid": "solid",
      "dashed": "dashed",
      "dotted": "dotted"
    }
  },
  {
    "prefix": "position",
    "declaration": "position: ${value}",
    "variations": {
      "relative": "relative",
      "absolute": "absolute"
    }
  },
  {
    "prefix": "size",
    "declaration": "font-size: ${value} !important",
    "variations": {
      "2xs": "10px",
      "xs": "12px",
      "sm": "14px",
      "md": "16px",
      "lg": "18px",
      "xl": "20px",
      "2xl": "24px",
      "3xl": "30px",
      "4xl": "36px",
      "5xl": "48px",
      "6xl": "60px",
      "7xl": "72px",
      "8xl": "96px",
      "9xl": "128px"
    }
  }
]