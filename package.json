{
  "name": "anubis-ui",
  "version": "1.2.21",
  "description": "Class-based css generator",
  "main": "index.js",
  "keywords": [
    "anubis",
    "anubisUI",
    "css",
    "scss",
    "style",
    "plugin",
    "vite",
    "quasar",
    "vue"
  ],
  "scripts": {
    "dev": "rm -rf dist; npm run prepare; node src/manual/build.js",
    "build": "node scripts/generate-version.js && tsc && tsc-alias",
    "preinstall": "npm run build",
    "prepare": "npm run build",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui"
  },
  "style": "dist/_anubis.scss",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HugoLMTY/anubis-ui.git"
  },
  "types": "./index.d.ts",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HugoLMTY/anubis-ui/issues"
  },
  "homepage": "https://github.com/HugoLMTY/anubis-ui#readme",
  "devDependencies": {
    "@types/node": "^22.14.1",
    "vite": "^6.3.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "fast-glob": "^3.3.3",
    "typescript": "^5.8.3",
    "tsc-alias": "^1.8.16"
  }
}
